        <Authors>modo.lv</Authors>
        <RepositoryUrl>https://github.com/modo-lv/Simpler.NetCore.Collections/tree/v0.0.8.1</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <PackageProjectUrl></PackageProjectUrl>
